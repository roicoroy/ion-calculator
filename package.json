{
  "name": "ion-calculator",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "assets:ios": "npx @capacitor/assets generate --ios",
    "assets:android": "npx @capacitor/assets generate --android",
    "easy-ios": "npx @capacitor/assets generate --assetPath './resources/ios' --iosProject 'ios/App' --iconBackgroundColor '#ffffff' --iconBackgroundColorDark '#000000' --splashBackgroundColor '#ffffff' --splashBackgroundColorDark '#000000'",
    "easy-and": "npx @capacitor/assets generate --assetPath './resources/android' --iosProject 'android' --iconBackgroundColor '#ffffff' --iconBackgroundColorDark '#000000' --splashBackgroundColor '#ffffff' --splashBackgroundColorDark '#000000'",
    "android": "ionic build && npx cap sync android && npx cap open android",
    "ios": "ionic build && npx cap sync ios && npx cap open ios",
    "li-ios": "ionic capacitor run ios -l --external",
    "li-and": "ionic capacitor run android -l --external",
    "d-f": "npx nx run nx-calculator:build && firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "18.2.7",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "18.2.7",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@capacitor/android": "^6.1.1",
    "@capacitor/app": "6.0.1",
    "@capacitor/app-launcher": "^6.0.1",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/camera": "^6.0.1",
    "@capacitor/core": "6.1.2",
    "@capacitor/device": "^6.0.0",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor/haptics": "6.0.1",
    "@capacitor/ios": "^6.1.1",
    "@capacitor/keyboard": "6.0.2",
    "@capacitor/share": "^6.0.1",
    "@capacitor/splash-screen": "^6.0.1",
    "@capacitor/status-bar": "6.0.1",
    "@capawesome-team/capacitor-file-opener": "^6.1.0",
    "@capawesome/capacitor-file-picker": "^6.1.0",
